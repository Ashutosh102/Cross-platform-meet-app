    "@sentry/tracing": "6.2.1",
    "agora-react-native-rtm": "1.5.0",
    "agora-rtc-sdk-ng": "4.14.0",
    "agora-rtm-sdk": "1.6.0-204",
    "electron-log": "4.3.5",
    "electron-squirrel-startup": "1.0.0",
    "electron-updater": "4.3.9",
    "exponential-backoff": "3.1.0",
    "graphql": "15.5.0",
